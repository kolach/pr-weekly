{{#> layout.hbs}}
{{#each pulls}}
  <div class="pr">
    <a href="{{ url }}">
      <h2>{{ title }}</h2>
    </a>
    {{ bodyHTML }}
    <div class="footer">
      {{#if isDraft}}
        <span class="draft">Draft</span>
      {{/if}}
      <span class="author">by: {{ author.login }}</span>
    </div>
  </div>
{{/each}}
{{/layout.hbs}}
